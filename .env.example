# RootzEngine Environment Configuration
# Copy this file to .env and update with your values

# Environment
ENVIRONMENT=development
DEBUG=true

# Audio Processing Mode
ROOTZ_USE_AZURE=false

# Azure Configuration (when using Azure)
AZURE_STORAGE_ACCOUNT=your_storage_account
AZURE_CONTAINER_NAME=rootzengine-data
AZURE_CONNECTION_STRING=your_connection_string

# Audio Settings (optional overrides)
AUDIO__SAMPLE_RATE=22050
AUDIO__HOP_LENGTH=512
AUDIO__N_FFT=2048

# Demucs Settings (optional overrides)
DEMUCS__MODEL_NAME=htdemucs_ft
DEMUCS__DEVICE=cpu